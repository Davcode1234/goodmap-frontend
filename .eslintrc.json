{
   "extends": ["airbnb", "prettier"],
   "plugins": ["prettier", "eslint-plugin-react", "eslint-plugin-react-hooks"],
   "parserOptions": {
      "sourceType": "module"
   },
   "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "import/prefer-default-export": "off"
   }
}
